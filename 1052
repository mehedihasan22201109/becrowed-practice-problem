#include<stdio.h>
int main()
{
    int M;
    scanf("%d", &M);

    if(M == 1)
    {
        printf("January\n");
    }
    else if (M == 2)
    {
        printf("February\n");
    }
    else if (M == 3)
    {
        printf("March\n");
    }
    else if (M == 4)
    {
        printf("April\n");
    }
    else if (M == 5)
    {
        printf("May\n");
    }
    else if (M == 6)
    {
        printf("June\n");
    }
    else if (M == 7)
    {
        printf("July\n");
    }
    else if (M == 8)
    {
        printf("August\n");
    }
    else if (M == 9)
    {
        printf("September\n");
    }
    else if (M == 10)
    {
        printf("October\n");
    }
    else if (M == 11)
    {
        printf("November\n");
    }
    else if (M == 12)
    {
        printf("December\n");
    }
    return 0;
}
