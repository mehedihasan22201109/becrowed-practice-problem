#include <stdio.h>
int main()
{
    int A,B,C=0,D=0,E=0,F=0;
    for(A=1; A<=5; A++)
    {
        scanf("%d", &B);
        if(B%2==0) C++;
        if(B%2==1||B%2==-1) D++;
        if(B>0) E++;
        if(B<0) F++;
    }
    printf("%d valor(es) par(es)\n",C);
    printf("%d valor(es) impar(es)\n",D);
    printf("%d valor(es) positivo(s)\n",E);
    printf("%d valor(es) negativo(s)\n",F);

    return 0;
}
